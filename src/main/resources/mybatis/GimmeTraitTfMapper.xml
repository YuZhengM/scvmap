<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.boot.mapper.GimmeTraitTfMapper">
    <resultMap id="BaseResultMap" type="com.spring.boot.pojo.GimmeTraitTf">
        <result column="f_trait_id" jdbcType="VARCHAR" property="traitId"/>
        <result column="f_tf" jdbcType="VARCHAR" property="tf"/>
        <result column="f_score" jdbcType="DOUBLE" property="score"/>
        <result column="f_score_mean" jdbcType="DOUBLE" property="scoreMean"/>
    </resultMap>
    <select id="selectByCoScoreAndMeanScore" resultType="java.lang.String">
        <foreach collection="signalIdTraitTfListMap" index="key" item="values" separator=" UNION ">
            SELECT f_trait_id FROM t_gimme_trait_tf_${key} WHERE f_tf IN
            <foreach collection="values" open="(" separator="," close=")" item="value">
                #{value}
            </foreach>
            <if test="coScore != null and coScore != 0">
                AND f_score <![CDATA[ >= ]]> #{coScore}
            </if>
            <if test="meanScore != null and meanScore != 0">
                AND f_score_mean <![CDATA[ >= ]]> #{meanScore}
            </if>
        </foreach>
    </select>
</mapper>
